{
    "$comment": "FreeCharts 流程图编辑器 - Conditional菱形识别测试",
    "$description": "使用正确尺寸的模板(0.45倍缩放)，无需多尺度匹配",
    "$resource_base": "../resources/freecharts",
    
    "开始测试": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "pre_delay": 1500,
        "next": ["识别菱形图标"]
    },
    
    "识别菱形图标": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/final/Conditional_fix.png"],
        "threshold": [0.2],
        "roi": [0, 100, 220, 700],
        "multi_scale": false,
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 500,
        "next": ["在画布放置菱形"]
    },
    
    "在画布放置菱形": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [600, 300],
        "pre_delay": 200,
        "post_delay": 500,
        "next": ["再次识别菱形"]
    },
    
    "再次识别菱形": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/final/Conditional_fix.png"],
        "threshold": [0.2],
        "roi": [0, 100, 220, 700],
        "multi_scale": false,
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 500,
        "next": ["在画布放置第二个菱形"]
    },
    
    "在画布放置第二个菱形": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [600, 500],
        "pre_delay": 200,
        "post_delay": 500,
        "next": ["第三次识别菱形"]
    },
    
    "第三次识别菱形": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/final/Conditional_fix.png"],
        "threshold": [0.2],
        "roi": [0, 100, 220, 700],
        "multi_scale": false,
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 500,
        "next": ["在画布放置第三个菱形"]
    },
    
    "在画布放置第三个菱形": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [800, 300],
        "pre_delay": 200,
        "post_delay": 500,
        "next": ["测试完成"]
    },
    
    "测试完成": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "next": []
    }
}
