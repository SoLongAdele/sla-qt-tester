{
    "$comment": "FreeCharts 流程图编辑器 - 完整功能测试",
    "$description": "测试创建完整流程图：开始→处理→判断→结束",
    "$resource_base": "../resources/freecharts",
    
    "开始测试": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "pre_delay": 1500,
        "next": ["等待应用启动"]
    },
    
    "等待应用启动": {
        "recognition": "TemplateMatch",
        "template": ["logo.png"],
        "threshold": [0.5],
        "roi": [0, 0, 1920, 200],
        "timeout": 10000,
        "rate_limit": 500,
        "action": "DoNothing",
        "post_delay": 500,
        "next": ["点击选择工具"]
    },
    
    "点击选择工具": {
        "recognition": "TemplateMatch",
        "template": ["pointer.png"],
        "threshold": [0.6],
        "roi": [0, 0, 300, 100],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 200,
        "post_delay": 300,
        "next": ["创建开始节点"]
    },
    
    "创建开始节点": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/StartEnd.png"],
        "threshold": [0.6],
        "roi": [0, 50, 200, 800],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 400,
        "next": ["放置开始节点"]
    },
    
    "放置开始节点": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [500, 150],
        "pre_delay": 200,
        "post_delay": 400,
        "next": ["创建处理节点"]
    },
    
    "创建处理节点": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/Step.png"],
        "threshold": [0.6],
        "roi": [0, 50, 200, 800],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 400,
        "next": ["放置处理节点"]
    },
    
    "放置处理节点": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [500, 300],
        "pre_delay": 200,
        "post_delay": 400,
        "next": ["创建判断节点"]
    },
    
    "创建判断节点": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/Conditional.png"],
        "threshold": [0.6],
        "roi": [0, 50, 200, 800],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 400,
        "next": ["放置判断节点"]
    },
    
    "放置判断节点": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [500, 480],
        "pre_delay": 200,
        "post_delay": 400,
        "next": ["创建结束节点"]
    },
    
    "创建结束节点": {
        "recognition": "TemplateMatch",
        "template": ["NodesIcon/StartEnd.png"],
        "threshold": [0.6],
        "roi": [0, 50, 200, 800],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 300,
        "post_delay": 400,
        "next": ["放置结束节点"]
    },
    
    "放置结束节点": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [500, 650],
        "pre_delay": 200,
        "post_delay": 500,
        "next": ["测试完成"]
    },
    
    "测试完成": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "next": []
    },
    
    "连线测试入口": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "pre_delay": 500,
        "next": ["点击连线工具"]
    },
    
    "点击连线工具": {
        "recognition": "TemplateMatch",
        "template": ["addline.png"],
        "threshold": [0.6],
        "roi": [0, 0, 400, 100],
        "timeout": 5000,
        "action": "Click",
        "target": true,
        "pre_delay": 200,
        "post_delay": 500,
        "next": ["绘制连线"]
    },
    
    "绘制连线": {
        "recognition": "DirectHit",
        "action": "Swipe",
        "begin": [500, 180],
        "end": [500, 270],
        "duration": 300,
        "post_delay": 500,
        "next": []
    }
}
